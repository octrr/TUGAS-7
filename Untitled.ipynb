    "def pembalik(teks):\n",
    "    list = []\n",
    "    hasil = ''\n",
    "    for i in range (len(teks)) :\n",
    "        list.append(teks[i])\n",
    "    while list != [] :\n",
    "        hasil += list.pop()\n",
    "    return hasil \n",
    "a = input(\"masukan teks :\")\n",
    "print(\"hasil : \", pembalik (a))"
   ]
